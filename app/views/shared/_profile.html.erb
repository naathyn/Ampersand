<% if @profile_items.any? %>
	<div class="well-header" style="margin-top:10px;"><%= pluralize(@user.profile.count, "share") %> by <%= @user.name %></div>
		<ol class="microposts">
			<%= render partial: 'shared/profile_item', collection: @profile_items %>
			</ol>
		<%= will_paginate @profile_items %>
	<div class="well-bottom" style="margin-bottom:15px;"></div>
<% end %>
