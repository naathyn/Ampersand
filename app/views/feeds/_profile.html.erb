<% if @profile_items.any? %>
  <div class="well-header" style="margin-top:10px;">
    <%= pluralize(@user.profile.count, "share") %> by <%= @user.name %>
      </div>
      <%= render partial: 'feeds/profile_item', collection: @profile_items %>
    <div class="well-bottom" style="margin-bottom:15px;"> </div>
  <%= will_paginate @profile_items %>
<% end %>
